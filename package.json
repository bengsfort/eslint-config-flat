{
  "name": "@bengsfort/eslint-config-flat",
  "description": "Flat version of my eslint config.",
  "author": "Matt Bengston <bengsfort@gmail.com> (https://bengsfort.dev)",
  "homepage": "https://github.com/bengsfort/eslint-config-flat",
  "version": "0.2.5",
  "bugs": {
    "url": "https://github.com/bengsfort/eslint-config-flat/issues"
  },
  "private": false,
  "repository": "github:bengsfort/stdlib",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./prettier.config.js": {
      "default": "./lib/prettier.config.js"
    }
  },
  "files": [
    "./lib",
    "./README.md",
    "./LICENSE.md",
    "./package.json"
  ],
  "scripts": {
    "add-change": "pnpm changeset",
    "bump-version": "pnpm changeset version",
    "ci:publish": "pnpm publish -r --access public"
  },
  "peerDependencies": {
    "eslint": "^9.20.1",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.0",
    "eslint": "^9.20.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@eslint/js": "^9.20.0",
    "@stylistic/eslint-plugin": "^4.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "prettier": "^3.5.1",
    "typescript-eslint": "^8.24.1"
  }
}
